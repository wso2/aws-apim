[server]
hostname = "<%= @hostname %>"
# offset  = 10

[user_store]
type = "read_only_ldap"

[keystore.primary]
file_name = "<%= @mi_key_store_location %>"
password = "<%= @key_store_password %>"
alias = "<%= @key_store_key_alias %>"
key_password = "<%= @key_store_key_password %>"

[truststore]
file_name = "<%= @mi_trust_store_location %>"
password = "<%= @trust_store_password %>"
alias = "symmetric.key.value"
algorithm = "AES"

